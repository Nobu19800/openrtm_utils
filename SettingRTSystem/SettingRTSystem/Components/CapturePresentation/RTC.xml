            </rtc:Constraint>
            <rtcDoc:Doc rtcDoc:constraint="1&lt;=x&lt;=200" rtcDoc:range="" rtcDoc:unit="" rtcDoc:description="キャプチャした画像に対する出力する画像データのサイズの比率" rtcDoc:defaultValue="2" rtcDoc:dataname="scale"/>
